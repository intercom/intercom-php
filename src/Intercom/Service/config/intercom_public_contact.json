{
    "name": "Intercom API (Contact)",
    "description": "The Intercom API (Contact)",
    "operations": {
        "getContact": {
            "httpMethod": "GET",
            "parameters": {
                "user_id": {
                    "location": "query",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "ContactModel",
            "responseType": "model",
            "summary": "Gets contact by user_id",
            "uri": "/contacts?user_id={user_id}"
        },
        "getContacts": {
            "extends": "_abstract_pagination_page",
            "httpMethod": "GET",
            "parameters": {
                "email": {
                    "location": "query",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "ContactListModel",
            "responseType": "model",
            "summary": "Gets contacts by email",
            "uri": "/contacts?email={email}"
        },
        "convertContact": {
          "httpMethod": "POST",
          "parameters": {
            "contact": {
              "location": "json",
              "required": true,
              "type": "array"
            },
            "user": {
              "location": "json",
              "required": true,
              "type": "array"
            }
          },
          "uri": "/contacts/convert",
          "responseClass": "ContactModel",
          "responseType": "model",
          "summary": "Converts a contact into a user"
        },
        "updateContact": {
            "httpMethod": "PUT",
            "parameters": {
                "id": {
                    "location": "json",
                    "required": true,
                    "type": "string"
                },
                "companies": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "created_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "custom_attributes": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "email": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "last_seen_ip": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_seen_user_agent": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "unsubscribed_from_emails": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                },
                "update_last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                },
                "user_id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "ContactModel",
            "responseType": "model",
            "summary": "Updates the contact",
            "uri": "/contacts"
        },
        "createContact": {
            "httpMethod": "POST",
            "parameters": {
                "id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "companies": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "created_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "custom_attributes": {
                    "location": "json",
                    "required": false,
                    "type": "array"
                },
                "email": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "integer",
                    "format": "timestamp"
                },
                "last_seen_ip": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "last_seen_user_agent": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "name": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "unsubscribed_from_emails": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                },
                "update_last_request_at": {
                    "location": "json",
                    "required": false,
                    "type": "boolean"
                }
            },
            "responseClass": "ContactModel",
            "responseType": "model",
            "summary": "Creates the contact",
            "uri": "/contacts"
        },
        "deleteContact": {
            "httpMethod": "DELETE",
            "parameters": {
                "id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                },
                "user_id": {
                    "location": "json",
                    "required": false,
                    "type": "string"
                }
            },
            "responseClass": "ContactModel",
            "responseType": "model",
            "summary": "Deletes a contact given id or user_id",
            "uri": "/contacts"
        }
    },
    "models": {
        "ContactModel": {
            "type": "object",
            "properties": {
                "anonymous": {
                  "location": "json",
                  "type": "boolean"
                },
                "pseudonym": {
                  "location": "json",
                  "type": "string"
                },
                "app_id": {
                    "location": "json",
                    "type": "string"
                },
                "avatar": {
                    "location": "json",
                    "type": "object",
                    "items": {
                        "image_url": {
                            "type": "string"
                        }
                    }
                },
                "companies": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "companies": {
                            "location": "json",
                            "type": "array",
                            "items": {
                                "$ref": "CompanyModel"
                            }
                        }
                    }
                },
                "created_at": {
                    "location": "json",
                    "type": "integer"
                },
                "custom_attributes": {
                    "location": "json",
                    "type": "object"
                },
                "email": {
                    "location": "json",
                    "type": "string"
                },
                "location_data": {
                    "location": "json",
                    "type": "object",
                    "items": {
                        "city_name": {
                            "type": "string"
                        },
                        "continent_code": {
                            "type": "string"
                        },
                        "country_code": {
                            "type": "string"
                        },
                        "latitude": {
                            "type": "integer"
                        },
                        "longitude": {
                            "type": "integer"
                        },
                        "postal_code": {
                            "type": "string"
                        },
                        "region_name": {
                            "type": "string"
                        },
                        "timezone": {
                            "type": "string"
                        }
                    }
                },
                "id": {
                    "location": "json",
                    "type": "string"
                },
                "last_request_at": {
                    "location": "json",
                    "type": "integer"
                },
                "name": {
                    "location": "json",
                    "type": "string"
                },
                "remote_created_at": {
                    "location": "json",
                    "type": "integer"
                },
                "signed_up_at": {
                    "location": "json",
                    "type": "integer"
                },
                "session_count": {
                    "location": "json",
                    "type": "integer"
                },
                "social_profiles": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "social_profiles": {
                            "location": "json",
                            "type": "array",
                            "items": {
                                "location": "json",
                                "type": "object",
                                "properties": {
                                    "type": {
                                        "type": "string"
                                    },
                                    "name": {
                                        "type": "string"
                                    },
                                    "id": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    },
                                    "url": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                },
                "segments": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "segments": {
                            "type": "array",
                            "location": "json",
                            "items": {
                                "$ref": "SegmentModel"
                            }
                        }
                    }
                },
                "tags": {
                    "location": "json",
                    "type": "object",
                    "properties": {
                        "tags": {
                            "type": "array",
                            "location": "json",
                            "items": {
                                "$ref": "TagModel"
                            }
                        }
                    }
                },
                "unsubscribed_from_emails": {
                    "location": "json",
                    "type": "boolean"
                },
                "updated_at": {
                    "location": "json",
                    "type": "integer"
                },
                "user_agent_data": {
                    "location": "json",
                    "type": "string"
                },
                "x_rate_limit": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Limit",
                    "type": "integer"
                },
                "x_rate_remaining": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Remaining",
                    "type": "integer"
                },
                "x_rate_reset": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Reset",
                    "type": "integer"
                }
            }
        },
        "ContactListModel": {
            "type": "object",
            "properties": {
                "contacts": {
                    "type": "array",
                    "location": "json",
                    "items": {
                        "$ref": "UserModel"
                    }
                },
                "x_rate_limit": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Limit",
                    "type": "integer"
                },
                "x_rate_remaining": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Remaining",
                    "type": "integer"
                },
                "x_rate_reset": {
                    "location": "header",
                    "sentAs": "X-RateLimit-Reset",
                    "type": "integer"
                }
            }
        }
    }
}
